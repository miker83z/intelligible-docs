{
  "src": [
    {
      "metadata-doc": ["./packages/metadata-doc/lib/*.js"],
      "intelligible-token-ethereum": [
        "./packages/Intelligible-Token-Ethereum/lib/*.js"
      ],
      "intelligible-identity": [
        "./packages/Intelligible-Identity-SDK/lib/*.js",
        "./packages/Intelligible-Identity-SDK/index.js"
      ],
      "intelligible-certificate": [
        "./packages/Intelligible-Certificate-SDK/lib/*.js",
        "./packages/Intelligible-Certificate-SDK/index.js"
      ]
    },
    {
      "guide": "./README.md"
    }
  ],
  "dest": "./output/docs",
  "app": {
    "title": "Intelligible Docs",
    "routing": "path",
    "entrance": "content:guide",
    "base": "/"
  },
  "template": {
    "path": "zebra",
    "options": {
      "title": {
        "label": "Docs",
        "href": "#"
      },
      "logo": {
        "dark": "img/logoblack.svg",
        "light": "img/logowhite.svg"
      },
      "sidebar": {
        "enabled": true,
        "outline": "tree",
        "collapsed": false,
        "toolbar": false,
        "itemsFolded": false,
        "itemsOverflow": "crop",
        "badges": true,
        "search": true,
        "animations": true
      },
      "symbols": {
        "autoLink": true,
        "params": "list",
        "enums": "list",
        "props": "list",
        "meta": false
      },
      "contentView": {
        "bookmarks": "h1,h2,h3",
        "faLibs": "solid,regular,brands"
      },
      "navbar": {
        "enabled": true,
        "dark": true,
        "animations": true,
        "menu": [
          {
            "label": "Docs",
            "iconClass": "fas fa-book",
            "items": [
              {
                "label": "metadata-doc",
                "href": "/api/metadata-doc/"
              },
              { "separator": true },
              {
                "label": "intelligible-token-ethereum",
                "href": "/api/intelligible-token-ethereum/"
              },
              { "separator": true },
              {
                "label": "intelligible-identity",
                "href": "/api/intelligible-identity/"
              },
              { "separator": true },
              {
                "label": "intelligible-certificate",
                "href": "/api/intelligible-certificate/"
              }
            ]
          },
          {
            "label": "Demo",
            "iconClass": "fas fa-mouse-pointer",
            "href": "https://demo.intelligible.io/",
            "target": "_blank"
          },
          {
            "label": "GitHub",
            "iconClass": "fab fa-github",
            "href": "https://github.com/BitNomos/Intelligible-Identity-SDK",
            "target": "_blank"
          }
        ]
      }
    }
  }
}
